<mvc:View
    controllerName="ui5.walkthrough.controller.Home"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}"
    class="homeBackground"
    titleAlignment="Center" >
        <content>
            <!-- 중앙 정렬을 위해 VBox 사용 -->
            <VBox
                justifyContent="Center"
                alignItems="Center"
                height="100%">
                <Image
                    
                    src="https://www.farnham.gov.uk/wp-content/uploads/2020/01/DBA-logo-for-social-networks-1200-x-630px-635x333.jpg"
                    alt="Logo"
                    press="onNavigateToInvoices"
                    class="homeLogo"
                />


            </VBox>
        </content>
    </Page>
</mvc:View>
